{
  "defaultVariables": null,
  "documentGenerator": {
    "fromDocument": {
      "json": "https://localhost:7246/swagger/v1/swagger.json",
      "url": "http://redocly.github.io/redoc/openapi.yaml",
      "output": "swagger.json",
      "newLineBehavior": "Auto"
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClients": {
      "className": "{controller}ServiceProxy",
      "template": "Angular",
      "httpClass": "HttpClient",
      "useSingletonProvider": true,
      "promiseType": "Promise",
      "injectionTokenType": "InjectionToken",
      "outputDir": "D:/Working/roomrentalmanagerclient/src/app/shared/services",
      "generateClientClasses": true
    }
  }
}
